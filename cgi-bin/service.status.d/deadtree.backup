DEADTREE=$(ssh -i /var/www/status/backup_check -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no backup_status@burn.za3k.com | grep deadtree)
echo $DEADTREE
EXPECTED=$(date +"deadtree.%Y%m%d")
[ "${DEADTREE}" = "${EXPECTED}" ] && exit 0
EXPECTED=$(date --date="yesterday"  +"deadtree.%Y%m%d")
[ "${DEADTREE}" = "${EXPECTED}" ] && exit 0
EXPECTED=$(date --date="2 days ago"  +"deadtree.%Y%m%d")
[ "${DEADTREE}" = "${EXPECTED}" ] && exit 0
